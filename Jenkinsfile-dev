pipeline {
    agent any
    tools{
        jdk 'java17'
        maven 'maven3'
    }
    stages {
        stage('maven Build ') {
            steps {
                echo 'it is dev  branch launched fr build'
                echo 'mvn clean install'
            }
        }    
        stage('maven test') {
            steps {
                echo 'it is dev  branch launched fr clean test'
                echo 'mvn clean test'
            }
        }
    }
}



