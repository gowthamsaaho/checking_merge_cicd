pipeline {
    agent any
    tools{
        jdk 'java17'
        maven 'maven3'
    }
    stages {
        stage('maven Build') {
            steps {
                echo 'it is prod  branch launched fr build'
                 sh 'mvn clean build'
            }
        }  

        stage('maven clean') {
            steps {
                echo 'it is prod  branch launched fr clean'
                sh 'mvn clean test'
            }
        }

        stage('maven install ') {
            steps {
                echo 'it is prod  branch launched'
                sh 'mvn clean install'
            }
        }
          stage('maven install2 ') {
            steps {
                echo 'it is prod  branch launched2'
                sh 'mvn clean install'
            }
        }
    }
}



